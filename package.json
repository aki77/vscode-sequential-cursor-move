{
  "name": "sequential-cursor-move",
  "displayName": "Sequential cursor move",
  "publisher": "aki77",
  "description": "Many movement commands into one command.",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aki77/vscode-sequential-cursor-move.git"
  },
  "bugs": {
    "url": "https://github.com/aki77/vscode-sequential-cursor-move/issues"
  },
  "activationEvents": [
    "onCommand:seq.home",
    "onCommand:seq.end"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "seq.home",
        "title": "Sequential cursor move to home",
        "enablement": "editorTextFocus"
      },
      {
        "command": "seq.end",
        "title": "Sequential cursor move to end",
        "enablement": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  }
}
